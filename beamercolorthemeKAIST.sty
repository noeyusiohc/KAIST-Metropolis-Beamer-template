\ProvidesPackage{beamercolorthemeKAIST}

% KAIST palette 
\definecolor{KAISTDarkBlue}{RGB}{0,65,145}   % PANTONE 286C
\definecolor{KAISTBlue}{RGB}{0,135,200}      % PANTONE 2925C
\definecolor{KAISTMediumBlue}{RGB}{0,65,135} % PANTONE 285C
\definecolor{KAISTLightBlue}{RGB}{95,190,235}% PANTONE 2985C
\definecolor{KAISTDarkGray}{RGB}{124,124,124}% PANTONE Cool Gray 8C
\definecolor{KAISTWhite}{RGB}{255,255,255}


% color setting
\setbeamercolor{title}{fg=KAISTDarkBlue}
\setbeamercolor{frametitle}{fg=KAISTWhite, bg=KAISTDarkBlue}
\setbeamercolor{structure}{fg=KAISTDarkBlue}
\setbeamercolor{progress bar}{fg=KAISTDarkBlue, bg=KAISTDarkGray}

% block setting
\setbeamercolor{block title}{fg=white, bg=KAISTDarkBlue} 
\setbeamercolor{block body}{fg=black, bg=KAISTBlue!10!white} 
\setbeamercolor{block title alerted}{fg=white, bg=red!70!black} 
\setbeamercolor{block body alerted}{fg=black, bg=red!10!white} 
\setbeamercolor{block title example}{fg=white, bg=KAISTBlue} 
\setbeamercolor{block body example}{fg=black, bg=KAISTLightBlue!20!white}

% palette primary 
\setbeamercolor{palette primary}{fg=KAISTDarkBlue, bg=KAISTBlue!15!white}

